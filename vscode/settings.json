{
    "telemetry.enableTelemetry": false,
    "editor.scrollBeyondLastLine": false,
    "explorer.autoReveal": false,
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#7288a8",
        "editor.wordHighlightBackground": "#3f2c33",
    },
    "workbench.settings.useSplitJSON": true,
    "gitlens.mode.active": "zen",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": false,
    "extensions.ignoreRecommendations": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/out": true,
        "**/.project": true,
        "**/.classpath": true,
        "**/.settings": true,
        "**/.gradle": true,
        "**/.idea": true,
        "**/.serverless": true,
        ".vstags": true,
        "**/.factorypath": true
    },
    "files.hotExit": "onExitAndWindowClose",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "diffEditor.renderSideBySide": false,
    "workbench.iconTheme": "material-icon-theme",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[perl]": {
        "editor.defaultFormatter": "Kaktus.perltidy-more"
    },
    "editor.minimap.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "gitlens.hovers.currentLine.over": "line",
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "foreground": "magenta"
        },
        "FIXME": {
            "foreground": "red"
        },
        "NOTE": {
            "foreground": "green"
        }
    },
    "todo-tree.highlights.defaultHighlight": {},
    "hadolint.hadolintPath": "/usr/local/bin/hadolint",
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "workbench.editor.limit.enabled": true,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "peacock.affectActivityBar": false,
    "files.associations": {
        "zshrc": "shellscript"
    },
    "yaml.schemas": {
        "file:///Users/albert.vila/.vscode/extensions/atlassian.atlascode-3.0.7/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "python.languageServer": "Default",
    "git.autofetch": true,
    "editor.renderControlCharacters": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "never"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.parquet": "default"
    },
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "json.maxItemsComputed": 500000,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "githubPullRequests.pullBranch": "never",
    "perltidy-more.executable": "/opt/homebrew/bin/perltidy",
    "perltidy-more.profile": "/Users/albert.vila/.perltidyrc",
    "databricks.connectionManager": "Databricks CLI Profiles",
    "databricks.lastActiveConnection": "DEFAULT",
    "jupyter.interactiveWindow.cellMarker.codeRegex": "^(#\\s*COMMAND\\s*[-]*|#\\s*%%|#\\s*\\<codecell\\>|#\\s*In\\[\\d*?\\]|#\\s*In\\[ \\])",
    "eslint.workingDirectories": [],
    "extensions.autoUpdate": false,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorTheme": "Solarized Dark",
    "window.zoomLevel": 1,
    "terminal.integrated.env.osx": {
        "CW_NEW_SESSION": "1"
    }
}
